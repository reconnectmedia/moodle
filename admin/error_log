            SELECT DISTINCT
                c.id AS course,
                cr.timeend AS timeend,
                cr.id AS criteriaid,
                ra.userid AS userid
            FROM
                mdl_course_completion_criteria cr
            INNER JOIN
                mdl_course c
             ON cr.course = c.id
            INNER JOIN
                mdl_context con
             ON con.instanceid = c.id
            INNER JOIN
                mdl_role_assignments ra
             ON ra.contextid = con.id
            LEFT JOIN
                mdl_course_completion_crit_compl cc
             ON cc.criteriaid = cr.id
            AND cc.userid = ra.userid
            WHERE
                cr.criteriatype = 2
            AND con.contextlevel = 50
            AND c.enablecompletion = 1
            AND cc.id IS NULL
            AND cr.timeend < ?
        
