            SELECT DISTINCT
                c.id AS course,
                cr.id AS criteriaid,
                ra.userid AS userid,
                cc.timecompleted AS timecompleted
            FROM
                mdl_course_completion_criteria cr
            INNER JOIN
                mdl_course c
             ON cr.course = c.id
            INNER JOIN
                mdl_context con
             ON con.instanceid = c.id
            INNER JOIN
                mdl_role_assignments ra
              ON ra.contextid = con.id
            INNER JOIN
                mdl_course_completions cc
             ON cc.course = cr.courseinstance
            AND cc.userid = ra.userid
            LEFT JOIN
                mdl_course_completion_crit_compl ccc
             ON ccc.criteriaid = cr.id
            AND ccc.userid = ra.userid
            WHERE
                cr.criteriatype = 8
            AND con.contextlevel = 50
            AND c.enablecompletion = 1
            AND ccc.id IS NULL
            AND cc.timecompleted IS NOT NULL
        
